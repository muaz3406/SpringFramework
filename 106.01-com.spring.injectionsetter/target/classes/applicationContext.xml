<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="civic" class="com.otostore.Civic">
		<property name="model" value="Civic"></property>
	</bean>
	
	<bean id="sahin" class="com.otostore.Sahin">
	<property name="marka" value="Tofas"/>
	<property name="model" value="Sahin"/>
	<property name="year" value="1985"/>
	</bean>
</beans>
